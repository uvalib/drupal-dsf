# Ignore vendor directories
/vendor/

# Ignore core directories
/web/core/

/web/libraries/

# Ignore contributed modules, themes and profiles
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/

/web/libraries/

# Ignore sensitive files
/web/sites/*/settings.php
/web/sites/*/settings.local.php
!web/sites/*/settings.ddev.php

# Ignore files generated in sites
/web/sites/*/files/css
/web/sites/*/files/js
/web/sites/*/files/php
/web/sites/*/files/styles
/web/sites/*/private/
/web/sites/*/translations/
/web/sites/*/tmp/

# DDEV
/.ddev/.gitignore
/.ddev/docker-compose*.yaml
/.ddev/db_snapshots/
/.ddev/import-db/
/.ddev/commands/
/web/.ddev-docker-compose*.yaml

# Ignore local dev files
.env
.env.local
.DS_Store
.idea/

# Ignore composer-managed finder module
web/modules/custom/finder

# Ignore composer-managed theme
web/themes/custom/uva_dsf_uswds

# VS Code
.vscode/
.history/
*.code-workspace
