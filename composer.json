{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "git@github.com:uvalib/CD-finder-uva.git"
        },
        {
            "type": "git",
            "url": "git@github.com:uvalib/uva_dsf_uswds.git"
                }
    ],
    "autoload": {
        "psr-4": {
            "Uvalib\\DrupalDsf\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "composer/installers": "^1.9",
        "drupal/better_normalizers": "^2.0@beta",
        "drupal/ckeditor": "^1.0",
        "drupal/color": "^1.0",
        "drupal/core": "^10.0",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-recommended": "^10.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/default_content_deploy": "^2.1",
        "drupal/devel": "^5.3",
        "drupal/hal": "^2.0",
        "drupal/paragraphs": "^1.15",
        "drupal/quickedit": "^1.0",
        "drupal/rdf": "^2.0",
        "drupal/restui": "^1.0",
        "drupal/stable": "^2",
        "drupal/uswds": "^3",
        "drush/drush": "^12.0",
        "uvalib/cd-finder-uva": "dev-master",
        "uvalib/uva_dsf_uswds": "dev-main"
    },
    "require-dev": {
        "drupal/core-dev": "^10.0",
        "drupal/upgrade_status": "^4.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": false
        },
        "platform": {
            "php": "8.1"
        }
    },
    "replace": {
        "open-telemetry/context": "*",
        "open-telemetry/api": "*",
        "open-telemetry/sdk": "*",
        "open-telemetry/sem-conv": "*",
        "open-telemetry/gen-otlp-protobuf": "*",
        "open-telemetry/exporter-otlp": "*"
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/modules/custom/finder": ["uvalib/cd-finder-uva"],
            "web/themes/custom/{$name}": ["uvalib/uva_dsf_uswds"],
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
